<div class="container">
  <p class="text-success"><%= notice %></p>

  <h1 class="mb-4">Funds</h1>

  <div id="funds">
    <% @funds.each do |fund| %>
      <div class="card mb-3">
        <%= render fund %>
        <div class="card-footer">
          <%= link_to "Show this fund", fund, class: "btn btn-primary" %>
        </div>
      </div>
    <% end %>
  </div>

  <%= link_to "New fund", new_fund_path, class: "btn btn-success" %>
</div>
